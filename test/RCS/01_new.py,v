@#!/usr/bin/env python3

from test_more import ok, eq
from graphtools import Graph

g = Graph(directed=False, multiedged=True)
ok(g)
ok(not g.directed())
ok(g.multiedged())

g = Graph(directed=True, multiedged=True)
ok(g)
ok(g.directed())
ok(g.multiedged())
